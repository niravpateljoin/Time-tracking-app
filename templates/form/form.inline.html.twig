{% block form_row -%}
    <div class="form-group row {{ form.vars.name }}{% if (not compound or force_error|default(false)) and not valid %} has-error{% endif %}">
        {{- form_label(form, null, { label_attr: { class: 'col-form-label col-lg-2' } }) -}}
        <div class="col-lg-10">
        {{- form_widget(form) -}}
        {{- form_errors(form) -}}
        </div>
    </div>
{%- endblock form_row %}